<section id="skillSets" class="panel">
    <% @skill_sets.each do |skill_set| %>
        <section class="skill-set">
            <%= link_to(skill_set_path(skill_set)) do %>
                <h3><%= skill_set.name %></h3>
                <% skill_set.skills.each do |skill| %>
                    <%= link_to(skill_path(skill)) do %>
                        <button class="skill-button"><%= skill.name %></button>
                    <% end %>
                <% end %>
            <% end %>
        </section>
    <% end %>
</section>
